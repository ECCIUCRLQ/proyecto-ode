    exec "/home/david/simics/simics-6.0.128/bin/test-runner" --package-list "/home/david/simics/ODEcam/.package-list" --project "/home/david/simics/ODEcam" "$@"
else
    exec "/home/david/simics/simics-6.0.128/bin/test-runner" --project "/home/david/simics/ODEcam" "$@"
fi
